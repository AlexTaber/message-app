<!DOCTYPE html>
<html>
<head>
  <title>MessageApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render partial: "layouts/header" %>
  <% if flash[:warn] %>
    <p><%= flash[:warn] %></p>
  <% end %>
  <% if flash[:notice] %>
    <p><%= flash[:notice] %></p>
  <% end %>
  <%= yield %>

</body>
</html>
